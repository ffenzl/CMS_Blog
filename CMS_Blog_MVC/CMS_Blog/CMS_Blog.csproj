    <PackageTargetFallback>portable-net45+win8</PackageTargetFallback>
    <RootNamespace>CMS_Blog</RootNamespace>
    <AssemblyName>CMS_Blog</AssemblyName>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="Views\Backend\Index.cshtml" />
